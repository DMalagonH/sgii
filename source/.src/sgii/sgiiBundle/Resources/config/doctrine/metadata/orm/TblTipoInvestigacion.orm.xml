      <generator strategy="IDENTITY"/>
    </id>
    <field name="tinNombreTipo" type="string" column="tin_nombre_tipo" length="250" nullable="false"/>
    <field name="tinEstado" type="boolean" column="tin_estado" nullable="false"/>
  </entity>
</doctrine-mapping>
